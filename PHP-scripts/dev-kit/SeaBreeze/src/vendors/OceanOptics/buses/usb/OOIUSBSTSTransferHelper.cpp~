#include "common/globals.h"
#include "vendors/OceanOptics/buses/usb/OOIUSBSTSTransferHelper.h"

using namespace seabreeze;

OOIUSBSTSTransferHelper::OOIUSBSTSTransferHelper(USB *usb,
        const OOIUSBSTSEndpointMap &map) : USBTransferHelper(usb) {

    this->sendEndpoint = map.getPrimaryOutEndpoint();
    this->receiveEndpoint = map.getPrimaryInEndpoint();
}

OOIUSBSTSTransferHelper::~OOIUSBSTSTransferHelper() {

}

